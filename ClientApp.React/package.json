{
  "name": "client-app-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-ts": "react-scripts-ts start",
    "build-ts": "react-scripts-ts build",
    "start": "npx env-cmd dev npm-run-all -p watch-css start-ts",
    "build": "npm-run-all build-css build-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "axios": "0.17.1",
    "bootstrap": "4.0.0",
    "material-ui": "^1.0.0-beta.39",
    "oidc-client": "1.4.1",
    "react": "16.3.0",
    "prop-types": "15.6.1",
    "react-dom": "16.3.0",
    "react-responsive": "4.1.0",
    "react-router-dom": "4.2.2",
    "react-scripts-ts": "2.14.0"
  },
  "devDependencies": {
    "@types/jest": "22.2.2",
    "@types/node": "^9.3.0",
    "@types/react": "16.1.0",
    "@types/prop-types": "15.5.2",
    "@types/react-dom": "16.0.4",
    "@types/react-responsive": "3.0.1",
    "@types/react-router-dom": "4.2.6",
    "env-cmd": "8.0.0",
    "jquery": "3.3.1",
    "npm-run-all": "4.1.2",
    "node-sass-chokidar": "1.2.2",
    "typescript": "2.8.1"
  }
}
